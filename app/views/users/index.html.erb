<h1>Welcome to WeWatch</h1>



<%= form_with model: @user do |f| %>
    
    <%= f.label :user_name, 'User Name:'%>
    <%= f.text_field :user_name %><br/>
    <%= f.label :password, 'Password: '%>
    <%= f.text_field :password %><br/>

    <%= f.submit "Sign In" %>
<% end %>

<p>New User? <%= link_to "Sign Up Here", new_user_path %><p>